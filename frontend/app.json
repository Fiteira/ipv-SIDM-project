{
  "expo": {
    "name": "Sensor Monitor",
    "slug": "sensor-monitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mamaguebo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "projectId": "3ecaa0d9-d31c-49ab-b43c-cf31f7617b1a",
    "splash": {
      "image": "./assets/mamaguebo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.sidm.sensormonitor",
        "googleServicesFile": "./google-services.json",
        "useNextNotificationsApi": true
      },
      "package": "com.sidm.sensormonitor"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3ecaa0d9-d31c-49ab-b43c-cf31f7617b1a"
      }
    }
  },
  "build": {
    "production": {
      "android": {
        "buildType": "apk"
      }
    }
  }
}
