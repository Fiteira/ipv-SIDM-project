{
  "expo": {
    "name": "Sensor Monitor",
    "slug": "sensor-monitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dreamy.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "projectId": "02322587-e706-4724-abac-66b4ea98a0c8",
    "splash": {
      "image": "./assets/dreamy.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.sidm.sensormonitor",
        "googleServicesFile": "./google-services.json",
        "useNextNotificationsApi": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
